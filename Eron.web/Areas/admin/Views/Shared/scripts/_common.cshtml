<script>
    $(document).ready(function() {
        toastr.options = {
            "closeButton": true,
            "debug": false,
            "progressBar": true,
            "preventDuplicates": false,
            "positionClass": "toast-bottom-left",
            "onclick": null,
            "showDuration": "4000",
            "hideDuration": "1000",
            "timeOut": "7000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
        }
        toastr.success("به پنل مدیریت خوش آمدید","کاربر گرامی");
    });

    $(".checkbox-addon").on('click', function() {
        if ($(this).val().is("on")) {
            $(this).val('true');
        }
    });
</script>
